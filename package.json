{
  "name": "chord-parser",
  "version": "1.0.1",
  "description": "Regex-based musical chord parser",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nolanlawson/chord-parser.git"
  },
  "keywords": [
    "chord",
    "parse",
    "music",
    "parser"
  ],
  "author": "Nolan Lawson <nolan.lawson@gmail.com>",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/nolanlawson/chord-parser/issues"
  },
  "homepage": "https://github.com/nolanlawson/chord-parser",
  "scripts": {
    "test": "npm run jshint && npm run test-node",
    "jshint": "jshint -c .jshintrc *.js test/test.js",
    "test-node": "istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100",
    "build": "mkdir -p dist && browserify index.js -s chord-parser -o dist/chord-parser.js && npm run min",
    "min": "uglifyjs dist/chord-parser.js -mc > dist/chord-parser.min.js"
  },
  "devDependencies": {
    "browserify": "~2.36.0",
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.1",
    "istanbul": "^0.2.7",
    "jshint": "^2.5.5",
    "mocha": "^1.18.2",
    "uglify-js": "^2.4.13"
  }
}
